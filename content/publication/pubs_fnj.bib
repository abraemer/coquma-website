issn = {10797114},
journal = {Physical Review Letters},
keywords = {doi:10.1103/PhysRevLett.122.050403 url:https://doi},
number = {5},
pages = {50403},
publisher = {American Physical Society},
title = {{Dynamical Topological Transitions in the Massive Schwinger Model with a $\theta$ Term}},
